{
    "name": "berndengels/laravel8-email-exceptions",
    "description": "Email Exceptions from your laravel v8 application",
    "license": "MIT",
    "keywords": ["email", "exception", "laravel", "laravel-8-package", "exception-handling", "debugging"],
    "authors": [
        {
            "name": "Aaron Brigham",
            "email": "abrigham1@gmail.com"
        },
        {
            "name": "Bernd Engels",
            "email": "engels@f50.de"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2|^8.0",
        "illuminate/support": "^7.0|^8.0",
        "jenssegers/agent": "^2.6"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "orchestra/testbench": "^5.8.0",
        "phpunit/phpunit": "^8.5.8|^9.3.3",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Bengels\\LaravelEmailExceptions\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bengels\\LaravelEmailExceptions\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bengels\\LaravelEmailExceptions\\EmailExceptionsServiceProvider"
            ]
        }
    },
    "scripts": {
        "run-tests": "./vendor/bin/phpunit"
    },
    "repositories": {
        "packagist.org": true,
        "private-packagist": false
    }
}
